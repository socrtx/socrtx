    <servlet-class>trabajoFinalGoogleComputing.MiAutenticaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MiAutentica</servlet-name>
    <url-pattern>/MiAutentica</url-pattern>
  </servlet-mapping>
  

  <servlet-mapping>
    <servlet-name>MiAutentica</servlet-name>
    <url-pattern>/MiAutentica</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  
</web-app>